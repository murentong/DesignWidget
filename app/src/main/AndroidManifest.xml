<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuacy.designwidget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".module.AppBarLayout.AppBarActivity" />

        <activity android:name=".module.CollapsingToolbarLayout.CollapsingToolbarActivity" />

        <activity android:name=".module.CoordinatorLayout.CoordinatorActivity" />

        <activity android:name=".module.FloatingActionButton.FloatingActionButtonActivity" />

        <activity android:name=".module.NavigationView.NavigationViewActivity" />

        <activity android:name=".module.Snackbar.SnackbarActivity" />

        <activity android:name=".module.TabLayout.TabActivity" />

        <activity android:name=".module.TextInputLayout.TextInputActivity" />

    </application>

</manifest>