<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuacy.designwidget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".module.behavior.BehaviorMainActivity" />

        <activity android:name=".module.design.DesignMainActivity" />

        <activity android:name=".module.design.CoordinatorLayout.CoordinatorActivity" />

        <activity android:name=".module.design.NavigationView.NavigationViewActivity" />

        <activity android:name=".module.design.Snackbar.SnackBarActivity" />

        <activity android:name=".module.design.TabLayout.TabActivity" />

        <activity android:name=".module.design.TextInputLayout.TextInputActivity" />

        <activity android:name=".module.behavior.bottomSheet.BottomSheetActivity"/>

        <activity android:name=".module.behavior.fab.FabBottomInOutActivity"/>

    </application>

</manifest>